@model AisMKIT.Models.ListOfEducations

@{
    ViewData["Title"] = "Create";
}

<h2>Добавление</h2>

<h4>Учебное заведение</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="INN" class="control-label"></label>
                <input asp-for="INN" class="form-control" />
                <span asp-validation-for="INN" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DomenNames" class="control-label"></label>
                <input asp-for="DomenNames" class="form-control" />
                <span asp-validation-for="DomenNames" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DateOfCreated" class="control-label"></label>
                <input asp-for="DateOfCreated" class="form-control" />
                <span asp-validation-for="DateOfCreated" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClUchZavedCategoryId" class="control-label"></label>
                <select asp-for="ClUchZavedCategoryId" class ="form-control" asp-items="ViewBag.ClUchZavedCategoryId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Назад</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
