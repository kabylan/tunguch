@model IEnumerable<AisMKIT.Models.ListOfEducations>

@{
    ViewData["Title"] = "Index";
}

<h2>Учебные заведения</h2>

<p>
    <a asp-action="Create">Добавить</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.INN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DomenNames)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfCreated)
            </th>
            <th>
                @Html.Label("Категория")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.INN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DomenNames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClUchZavedCategory.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Редактирование</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Детали</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Удалить</a>
                @*<form asp-action="Delete" asp-route-id="@role.Id" method="post">
                    <button type="submit" class="btn btn-sm btn-danger">
                        Delete
                    </button>
                </form>*@
            </td>
        </tr>
}
    </tbody>
</table>
