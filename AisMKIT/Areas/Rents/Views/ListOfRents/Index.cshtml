@model IEnumerable<AisMKIT.Models.ListOfRents>

@{
    ViewData["Title"] = "Объекты для предоставления";
}

<h2>Объекты для предоставления</h2>
<br />
<p>
    <a asp-action="Create">Добавить новый</a>
</p>
<br />

<div class="overflow-auto">

    <table id="myTable" class="table table-hover">
        <thead class="thead-light">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NameRus)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.NameKyrg)
                    </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.DictRentObjectType)
                </th>
                @*<th>
                        @Html.DisplayNameFor(model => model.CreateDate)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ApplicationUser)
                    </th>*@
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.NameRus)
                    </td>
                    @*<td>
                            @Html.DisplayFor(modelItem => item.NameKyrg)
                        </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.DictRentObjectType.NameRus)
                    </td>
                    @*<td>
                            @Html.DisplayFor(modelItem => item.CreateDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ApplicationUser.Id)
                        </td>*@
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id">Редактировать</a> |
                        <a asp-action="Details" asp-route-id="@item.Id">Детали</a> |
                        <a asp-action="Delete" asp-route-id="@item.Id">Удалить</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>