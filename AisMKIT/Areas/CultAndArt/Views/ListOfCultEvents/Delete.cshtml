}

<h1>Удаление</h1>

      <h3>Вы уверены, что хотите удалить это?</h3>
<div>
    <h4>Мероприятие</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DictTypeOfKMM)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DictTypeOfKMM.Name)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DistLoc)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DistLoc.Name)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EventTopic)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EventTopic)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StartDateTime)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StartDateTime)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EndDateTime)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EndDateTime)
